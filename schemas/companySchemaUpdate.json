{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "The root schema",
  "description": "The root schema comprises the entire JSON document.",
  "default": {},
  "examples": [
    {
      "name": "Chipotle",
      "num_employees": 2050,
      "description": "burritos for everyone",
      "logo_url": "wwww.chipo.tle"
    }
  ],
  "required": [],

  "properties": {
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The name schema",
      "description": "An explanation about the purpose of this instance.",
      "default": "",
      "examples": ["Chipotle"]
    },
    "num_employees": {
      "$id": "#/properties/num_employees",
      "type": "integer",
      "minimum": 0,
      "title": "The num_employees schema",
      "description": "An explanation about the purpose of this instance.",
      "default": 0,
      "examples": [2050]
    },
    "description": {
      "$id": "#/properties/description",
      "type": "string",
      "title": "The description schema",
      "description": "An explanation about the purpose of this instance.",
      "default": "",
      "examples": ["burritos for everyone"]
    },
    "logo_url": {
      "$id": "#/properties/logo_url",
      "type": "string",
      "format": "uri",
      "title": "The logo_url schema",
      "description": "An explanation about the purpose of this instance.",
      "default": "",
      "examples": ["wwww.chipo.tle"]
    },
    "_token": {
      "$id": "#/properties/_token",
      "type": "string",
      "title": "The _token schema",
      "description": "An explanation about the purpose of this instance.",
